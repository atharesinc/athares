{
  "expo": {
    "name": "Athares",
    "slug": "athares",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Athares-icon-mask.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#282A38"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "web": {
      "name": "Athares"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "useNextNotificationsApi": true
    },
    "notification": {
      "icon": "./assets/images/Athares-owl-logo-large-white-thin.png",
      "color": "#282A38"
    }
  }
}
